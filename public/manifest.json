{
    "manifest_version": 2,
    "name": "Easy Weather Tab",
    "description": "A Chrome Tab extension based on http://weather.keepiteasy/net.",
    "version": "0.1",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "geolocation"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    }
}
