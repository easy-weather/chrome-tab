{
    "manifest_version": 2,
    "name": "Easy Weather Tab",
    "description": "A Chrome Tab extension based on http://weather.keepiteasy/net.",
    "version": "0.1",
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://maps.googleapis.com https://maps.gstatic.com; object-src 'self'",
    "permissions": [
        "tabs"
    ],
    "chrome_url_overrides" : {
        "newtab": "public/index.html"
    }
}
